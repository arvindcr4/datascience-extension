{
  "manifest_version": 3,
  "name": "Job Project Suggester",
  "description": "Suggests practice projects based on job listings using Gemini.",
  "version": "1.0",
  "permissions": [
    "activeTab",
    "storage",
    "scripting"
  ],
  "host_permissions": [
    "https://generativelanguage.googleapis.com/*"
  ],
  "action": {
    "default_popup": "popup.html"
  }
}
